_KNOTEXT	CSEG

_LMUL::
	push    dx
	push    ax
	mul     dx
	xchg    ax, bx
	mul     cx
	add     bx, ax
	pop     ax
	mul     cx
	add     bx, dx
	pop     dx
	ret
